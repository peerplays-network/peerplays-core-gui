{
  "name": "peerplays-core-gui",
  "version": "0.4.0",
  "description": "Advanced wallet interface for graphene based blockchains (Bishares, Muse..)",
  "homepage": "https://github.com/PBSA/peerplays-ui#readme",
  "author": "Cryptonomex, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PBSA/peerplays-core-gui/issues"
  },
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/PBSA/peerplays-core-gui"
  },
  "scripts": {
    "stylelint": "stylelint '**/*.scss'",
    "pre-release-s1": "rm -rf releases; rm -rf electron/releases; npm run electron; npm run postelectron;",
    "pre-release-s2": "cd electron/node_modules/electron-prebuilt; rm -rf dist;",
    "pre-release-all": "npm run pre-release-s1 && npm run pre-release-s2",
    "post-release": "cp -a electron/releases/. releases; echo 'Release builds can be found in ./releases directory';",
    "mac-r": "cd electron/node_modules/electron-prebuilt; npm_config_platform=darwin npm run postinstall; cd ../../; npm run release",
    "win-r": "cd electron/node_modules/electron-prebuilt; npm_config_platform=win32 npm run postinstall; cd ../../; npm run release-windows",
    "lin-r": "cd electron/node_modules/electron-prebuilt; npm_config_platform=linux npm run postinstall; cd ../../; npm run release-linux",
    "pre-mwl-1": "npm run pre-release-s2; npm run mac-r",
    "pre-mwl-2": "npm run pre-release-s2; npm run win-r",
    "pre-mwl-3": "npm run pre-release-s2; npm run lin-r",
    "mwl": "npm run pre-mwl-1 && npm run pre-mwl-2 && npm run pre-mwl-3",
    "release-mac": "npm run pre-release-all && npm run mac-r && npm run post-release;",
    "release-win": "npm run pre-release-all && npm run win-r && npm run post-release;",
    "release-lin": "npm run pre-release-all && npm run lin-r && npm run post-release;",
    "release-mwl": "npm run pre-release-s1 && npm run mwl && npm run post-release",
    "test-old": "./node_modules/.bin/mocha --compilers coffee:coffee-script,js:babel/register --require coffee-script/register",
    "test": "jest",
    "profile-dev": "webpack --config config/webpack-dev.js --profile --json > stats-dev.json",
    "profile-build": "NODE_ENV=production webpack --config config/webpack-deploy-prod.js --profile --json > stats-prod.json",
    "profile-nougly": "NODE_ENV=production webpack --config config/webpack-prod-nougly.js --profile --json > stats-prod-nougly.json",
    "debug": "node --inspect server.js",
    "start": "node server.js",
    "start-dev-net": "DEV_NET=true node server.js",
    "start-prod": "PROD=true node server.js",
    "build": "NODE_ENV=production webpack --config config/webpack-deploy-prod.js --progress --display-error-details --colors",
    "build-devnet": "NODE_ENV=production webpack --config config/webpack-build-dev-net.js --progress --display-error-details --colors",
    "build-dev": "NODE_ENV=production webpack --config config/webpack-deploy-dev.js --progress --display-error-details --colors",
    "build-prod": "NODE_ENV=production webpack --config config/webpack-deploy-prod.js --progress --display-error-details --colors",
    "build-remote-dev": "NODE_ENV=production webpack --config config/webpack-deploy-remote.js --progress --display-error-details --colors",
    "electron": "NODE_ENV=production webpack --config config/webpack-electron.js --progress --display-error-details --colors;",
    "postelectron": " echo 'copying to electron/build..'; cp -r build/* electron/build/; echo 'done.'",
    "build-win32": "set NODE_ENV=production && webpack --config config/webpack-deploy-prod.js --progress --colors &&  echo 'copying to electron/build..' &&  xcopy dist ..\\electron\\build /s /e /y && echo 'done.'",
    "build-electron-win": "set NODE_ENV=production && webpack --config config/webpack-electron.js --progress --colors &&  echo 'copying to electron/build..' &&  xcopy dist ..\\electron\\build /s /e /y && echo 'done.'"
  },
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "ReconnectingWebSocket": "git+https://github.com/svk31/reconnecting-websocket.git",
    "alt": "^0.17.8",
    "alt-container": "^1.0.0",
