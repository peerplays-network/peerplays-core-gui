  "name": "peerplays-core-gui",
  "version": "1.5.4",
  "description": "Advanced wallet interface for graphene based blockchains (Bitshares, Muse..)",
  "homepage": "./",
  "author": "Cryptonomex, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peerplays-network/peerplays-core-gui/issues"
  },
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/peerplays-network/peerplays-core-gui"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 11",
    "not op_mini all"
  ],
  "scripts": {
    "init": "npm install commitizen -g && npm i",
    "commit": "git-cz",
    "release": "standard-version",
    "test-old": "./node_modules/.bin/mocha --compilers coffee:coffee-script,js:babel/register --require coffee-script/register",
    "test": "jest",
    "debug": "node --inspect scripts/start.js",
    "start": "node scripts/start.js",
    "start-prod": "PROD=true node server.js",
    "stylelint": "stylelint '**/*.scss'",
    "stylelint:fix": "stylelint '**/*.scss' --fix",
    "sonar": "node scripts/sonar.js",
    "build": "node scripts/build.js",
    "electron": "electron build/electron.js",
    "make-build": "mkdir -p build",
    "prep-dist": "npm run make-build && cp electron.js build/",
    "dist": "npm run prep-dist && electron-builder -wm",
    "dist-linux": "npm run prep-dist && electron-builder -l",
    "live": "ELECTRON_IS_DEV=1 electron electron.js",
    "postinstall": "electron-builder install-app-deps"
  },
