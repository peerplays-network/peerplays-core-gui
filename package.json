{
  "name": "peerplays-core-gui",
  "version": "0.5.1",
  "description": "Advanced wallet interface for graphene based blockchains (Bitshares, Muse..)",
  "homepage": "https://github.com/peerplays-network/peerplays-ui#readme",
  "author": "Cryptonomex, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peerplays-network/peerplays-core-gui/issues"
  },
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/peerplays-network/peerplays-core-gui"
  },
  "scripts": {
    "init": "npm install commitizen -g && npm i",
    "commit": "git-cz",
    "pre-release": "npm run release -- --prerelease rc",
    "release": "standard-version",
    "release-major": "npm run release -- --release-as major",
    "release-minor": "npm run release -- --release-as minor",
    "release-patch": "npm run release -- --release-as patch",
    "stylelint": "stylelint '**/*.scss'",
    "test-old": "./node_modules/.bin/mocha --compilers coffee:coffee-script,js:babel/register --require coffee-script/register",
    "test": "jest",
    "debug": "node --inspect server.js",
    "start": "node scripts/start.js",
    "start-prod": "PROD=true node server.js",
    "build": "node scripts/build.js",
    "electron": "electron build/electron.js",
    "make-build": "mkdir -p build",
    "copy:electron": "npm run make-build && cp electron.js build/",
    "compile": "npm run copy:electron",
    "dist": "npm run compile && electron-builder -wm",
    "dist-linux": "npm run compile && electron-builder -l",
    "live": "npm run copy:electron && ELECTRON_IS_DEV=1 electron build/electron.js",
    "postinstall": "electron-builder install-app-deps"
  },
