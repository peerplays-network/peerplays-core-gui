  "scripts": {
    "test": "jest",
    "profile-dev": "webpack --config conf/webpack-dev.js --profile --json > stats-dev.json",
    "profile-build": "NODE_ENV=production webpack --config conf/webpack-deploy-prod.js --profile --json > stats-prod.json",
    "profile-nougly": "NODE_ENV=production webpack --config conf/webpack-prod-nougly.js --profile --json > stats-prod-nougly.json",
    "start": "node server.js",
    "start-dev-net": "DEV_NET=true node server.js",
    "start-prod": "PROD=true node server.js",
    "build": "NODE_ENV=production webpack --config conf/webpack-deploy-prod.js --progress --display-error-details --colors",
    "build-devnet": "NODE_ENV=production webpack --config conf/webpack-build-dev-net.js --progress --display-error-details --colors",
    "build-dev": "NODE_ENV=production webpack --config conf/webpack-deploy-dev.js --progress --display-error-details --colors",
    "build-prod": "NODE_ENV=production webpack --config conf/webpack-deploy-prod.js --progress --display-error-details --colors",
    "build-remote-dev": "NODE_ENV=production webpack --config conf/webpack-deploy-remote.js --progress --display-error-details --colors",
    "electron": "NODE_ENV=production webpack --config conf/webpack-electron.js --progress --display-error-details --colors;",
    "postelectron": " echo 'copying to electron/build..'; cp -r dist/* ../electron/build/; echo 'done.'",
    "build-win32": "set NODE_ENV=production && webpack --config conf/webpack-deploy-prod.js --progress --colors &&  echo 'copying to electron/build..' &&  xcopy dist ..\\electron\\build /s /e /y && echo 'done.'",
    "build-electron-win": "set NODE_ENV=production && webpack --config conf/webpack-electron.js --progress --colors &&  echo 'copying to electron/build..' &&  xcopy dist ..\\electron\\build /s /e /y && echo 'done.'"
  },
