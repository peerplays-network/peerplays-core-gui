{
  "env": {
    "development": {
      "plugins": ["transform-es2015-modules-commonjs"],
      "extra": {
        "react-transform": {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }
      }
    },
    "test": {
      "presets": ["babel-preset-react", "stage-0"],
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}